{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@8.0.7",
      "destination": "wwwroot/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "select2@4.1.0-rc.0",
      "destination": "wwwroot/select2/"
    },
    {
      "library": "flatpickr@4.6.13",
      "destination": "wwwroot/lib/flatpickr/",
      "files": [
        "dist/flatpickr.min.css",
        "dist/flatpickr.min.js"
      ]
    }
  ]
}